{#
/**
 * @file exo-icon-icon-list.html.twig
 * Default theme implementation to present a list of Micon icons/svgs.
 *
 * @see template_preprocess_exo_icon_list()
 *
 * @ingroup themeable
 */
#}

{%
  set classes = [
    'exo-icon-list',
  ]
%}
{% if content %}
  {{- content -}}
{% endif %}
<ul{{ attributes.addClass(classes) }}>
  {% for icon in icons  %}
    <li>{{ icon }}</li>
  {% endfor %}
</ul>
